{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "es6"],
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
}
